<?page title="JX_Market" contentType="text/html;charset=UTF-8"?>
<zk xmlns:html="http://www.w3.org/1999/xhtml">
<window id="wEAAC" apply="pe.com.jx_market.controller.PO_EAAdministrateAreaCreate" sizable="true"
        width="500px" height="540px" position="center,center" closable="true"
        action="show: slideDown;hide: slideUp">
    <style src="s-style.css"/>
    <vbox height="100%" width="100%" sclass="s-vbox-popup">
        <div align="center" height="10%">
            <label zclass="z-south-header" sclass="s-header">
                ${labels.pe.com.jx_market.eAAdministrateAreaCreate.Title}
            </label>
        </div>
        <grid width="100%" fixedLayout="true" sclass="s-grid-popup">
            <auxhead>
                <auxheader label="${labels.pe.com.jx_market.eAAdministrateAreaCreate.SubTitle}" colspan="2" />
            </auxhead>
            <columns>
                <column hflex="1" />
                <column hflex="1" />
            </columns>
            <rows>
                <row>
                    <label value="${labels.pe.com.jx_market.eAAdministrateAreaCreate.txtAreaName.Label}" />
                    <textbox id="txtAreaName" width="60%" />
                </row>
            </rows>
        </grid>
        <hbox align="end" pack="end" hflex="1" vflex="1" sclass="s-hbox-popup">
            <button id="btnSave" label="${labels.pe.com.jx_market.eAAdministrateAreaCreate.btnSave.Label }"
                image="/media/filesave.png" />
            <button id="btnClose" label="${labels.pe.com.jx_market.eAAdministrateAreaCreate.btnClose.Label }"
                image="/media/cancel.png" />
        </hbox>
    </vbox>
</window>
</zk>