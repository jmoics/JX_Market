                <label zclass="z-south-header" sclass="s-header">
                    ${labels.pe.com.jx_market.eAPricelist.Pricelist.Title}
                </label>
            </div>
        </north>
        <center autoscroll="true" sclass="s-center-popup">
            <listbox id="lstPricelist" sclass="employee-list" vflex="1" hflex="1">
                <listhead>
                    <listheader label="NÂº" id="headNumber" hflex="1"/>
                    <listheader label="${labels.pe.com.jx_market.eAPricelist.validFrom.Label}" hflex="1" />
                    <listheader label="${labels.pe.com.jx_market.eAPricelist.validTo.Label}" hflex="1" />
                    <listheader label="${labels.pe.com.jx_market.eAPricelist.currency.Label}" hflex="1" />
